\r\n  // * Permet de chercher la quantité dans le lien et de l'extraire\r\n  const qty = props.location.search\r\n    ? Number(props.location.search.split(\"=\")[1])\r\n    : 1;\r\n  const dispatch = useDispatch();\r\n  const removeFromCardHandler = (productId) => {\r\n    dispatch(removeFromCart(productId));\r\n  };\r\n  const checkoutHandler = () => {\r\n    props.history.push(\"/signin?redirect=shipping\");\r\n  };\r\n\r\n  useEffect(() => {\r\n    if (productId) {\r\n      dispatch(addToCart(productId, qty));\r\n    }\r\n  }, []);\r\n\r\n  return (\r\n    <div className=\"row top\">\r\n      <div className=\"col-2\">\r\n        <h1 className=\"cart-title\">Shopping Cart</h1>\r\n\r\n        {cartItems.length === 0 ? (\r\n          <MessageBox>\r\n            Cart is empty. <Link to=\"/\">Go Shopping</Link>\r\n          </MessageBox>\r\n        ) : (\r\n          <ul className=\"card\">\r\n            {cartItems.map((item) => (\r\n              <li key={item.product}>\r\n                <div className=\"row product-list\">\r\n                  <div>\r\n                    <img src={item.image} alt={item.name} className=\"small\" />\r\n                  </div>\r\n                  <div className=\"min-30 font-list\">\r\n                    <Link to={\"/product/\" + item.product}>{item.name}</Link>\r\n                  </div>\r\n                  <div className=\"flex\">\r\n                    <select\r\n                      value={item.qty}\r\n                      className=\"font-list\"\r\n                      onChange={(e) =>\r\n                        dispatch(addToCart(item.product, e.target.value))\r\n                      }\r\n                    >\r\n                      {[...Array(item.countInStock).keys()].map((x) => (\r\n                        <option key={x} value={x + 1}>\r\n                          {x + 1}\r\n                        </option>\r\n                      ))}\r\n                    </select>\r\n                    <div>\r\n                      <button\r\n                        type=\"button\"\r\n                        className=\"font-list\"\r\n                        onClick={() => removeFromCardHandler(item.product)}\r\n                      >\r\n                        Delete\r\n                      </button>\r\n                    </div>\r\n                  </div>\r\n                  <div className=\"font-list\">${item.price}</div>\r\n                </div>\r\n              </li>\r\n            ))}\r\n          </ul>\r\n        )}\r\n      </div>\r\n      <div className=\"col-1\">\r\n        <div className=\"card card-body\">\r\n          <ul>\r\n            <li>\r\n              <h2>\r\n                Subtotal ( {cartItems.reduce((a, c) => a + c.qty, 0)} items ) :\r\n                ${cartItems.reduce((a, c) => a + c.price * c.qty, 0)}\r\n              </h2>\r\n            </li>\r\n            <li>\r\n              <button\r\n                type=\"button\"\r\n                onClick={checkoutHandler}\r\n                className=\"primary block\"\r\n                disabled={cartItems.length === 0}\r\n              >\r\n                Proceed to Checkout\r\n              </button>\r\n            </li>\r\n          </ul>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n",["145","146"],"C:\\Users\\Youri\\Documents\\Apprendre\\React JS Works\\Full E-commerce\\Amazona\\frontend\\src\\actions\\productActions.js",[],"C:\\Users\\Youri\\Documents\\Apprendre\\React JS Works\\Full E-commerce\\Amazona\\frontend\\src\\constants\\cartConstants.js",[],"C:\\Users\\Youri\\Documents\\Apprendre\\React JS Works\\Full E-commerce\\Amazona\\frontend\\src\\actions\\userActions.js",[],"C:\\Users\\Youri\\Documents\\Apprendre\\React JS Works\\Full E-commerce\\Amazona\\frontend\\src\\reducers\\userReducers.js",[],"C:\\Users\\Youri\\Documents\\Apprendre\\React JS Works\\Full E-commerce\\Amazona\\frontend\\src\\screens\\SignInScreen.js",[],"C:\\Users\\Youri\\Documents\\Apprendre\\React JS Works\